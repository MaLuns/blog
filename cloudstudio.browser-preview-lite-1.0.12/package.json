{
    "name": "browser-preview-lite",
    "displayName": "Browser Preview Lite",
    "description": "",
    "version": "1.0.12",
    "publisher": "cloudstudio",
    "engines": {
        "vscode": "^1.33.1"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "repository": "internal",
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "preview.start",
                "title": "Open Preview Tab",
                "category": "Preview"
            },
            {
                "command": "preview.generateYml",
                "title": "Generate Preview Config File",
                "category": "Preview"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "webpack --mode production --config extension.webpack.config.js && yarn run build-webview",
        "watch": "webpack --mode development --watch --info-verbosity verbose --config extension.webpack.config.js",
        "build-webview": "cd webView && yarn run build ",
        "postinstall": "cd webView && yarn",
        "test": "yarn run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "2.2.42",
        "@types/node": "10.12.21",
        "tslint": "5.12.1",
        "typescript": "3.3.1",
        "vscode": "^1.1.36"
    },
    "dependencies": {
        "@types/http-proxy": "^1.17.0",
        "@types/js-yaml": "^3.12.1",
        "@types/portscanner": "^2.1.0",
        "@types/request": "^2.48.1",
        "del": "^5.1.0",
        "event-stream": "^4.0.1",
        "gulp": "^4.0.2",
        "gulp-filter": "^6.0.0",
        "gulp-sourcemaps": "^2.6.5",
        "gulp-typescript": "^5.0.1",
        "http-proxy": "^1.18.0",
        "js-yaml": "^3.13.1",
        "portscanner": "^2.2.0",
        "request": "^2.88.0",
        "run-sequence": "^2.2.1",
        "vsce": "^1.69.0",
        "vscode-nls": "^4.1.1",
        "vscode-nls-dev": "^3.3.1",
        "webpack": "4.28.4"
    }
}
