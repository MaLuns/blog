<header class="header">
    <nav id="header-nav" class="header-nav">
        <ul class="menu"><% for (const key in theme.menu) { %>
            <li class="menu-item">
                <a href="<%- url_for(theme.menu[key])%>"><%= __(`menu.${key}`) %></a>
            </li> <% } %>
        </ul>
    </nav>
    <div class="header-title">
        <span>Hello World !</span>
    </div>
    <div class="header-user">
        <img class="user-avatar" src="<%- url_for(theme.user.avatar) %>" />
        <div>
            <%- theme.user.desc %>
        </div>
    </div>
</header>