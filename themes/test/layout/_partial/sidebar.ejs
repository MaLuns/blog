<div>
    <img src="https://diygod.me/images/card.jpg">
    <% if(theme.rss){%>
        <div class="feed-link">
            <a href="<%- url_for(theme.rss)%>" rel="alternate">
                <i class="fa fa-rss"></i>
                RSS
            </a>
        </div>
    <%}%>
    <div class="blog-class">
        <div>
            <a href="<%- url_for(theme.menu.archives)%>">
                <%- site.posts.length %>
                <p class="log">日志</p>
            </a>
        </div>
        <div class="line"></div>
        <div>
            <a href="<%- url_for(theme.menu.category)%>">
                <%- site.categories.length %>
                <p class="log">分类</p>
            </a>
        </div>
    </div>
</div>