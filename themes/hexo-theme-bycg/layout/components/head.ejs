<head>
    <meta http-equiv="content-type" content="text/html; charset=utf-8">
    <meta name="x5-fullscreen" content="true">
    <meta name="full-screen" content="yes">
    <meta name="theme-color" content="#317EFB"/>
    <meta content="width=device-width, initial-scale=1.0, maximum-scale=5.0, user-scalable=0" name="viewport">
    <% if (page.keywords){ %>
    <meta name="keywords" content="<%= page.keywords %>,<%= config.keywords %>">
    <% } else if (config.keywords){ %>
    <meta name="keywords" content="<%= config.keywords %>">
    <%} %>
    <% if(config.description){%>
    <meta name="description" content="<%- config.description %>">
    <%}%>
    <title><%= config.title %></title>
    <link rel="icon" type="image/png" sizes="32x32" href="<%= url_for(theme.favicon.narmal) %>?v=<%= theme.version %>">
    <link rel="stylesheet" href="//at.alicdn.com/t/font_1518765_srqlo2vvmu.css">
    <link rel="manifest" href="/manifest.json">
    <link rel="apple-touch-icon" href="/android-chrome-192x192.png">
    <%- css('css/highlight/highlight') %>
    <%- css('css/style.css?v='+theme.version) %>
</head>